@model Dictionary<int, decimal>
@{
    List<int> allCmids = (List<int>)ViewBag.AllCmids;
    List<int> bannedCmids = (List<int>)ViewBag.BannedCmids;
    List<int> notBannedCmids = (List<int>)ViewBag.NotBannedCmids;
    decimal threshold = (decimal)ViewBag.Threshold;
}
<p style="margin-top: 15px">
    You entered <span class="bold">@allCmids.Count</span> Cmids: I permanently banned
    and notified <span class="bold">@bannedCmids.Count</span> Cmids.
</p>
@if (notBannedCmids.Count > 0)
{
    <p style="margin-top: 10px">
        I was not able to ban the following Cmids (most likely admin or missing users):
    </p>
    <ul>
        @foreach (int cmid in notBannedCmids)
        {
            <li><a href='@Url.Action("See", "Member")?cmid=@cmid' target="_blank">@cmid</a></li>
        }
    </ul>
}
@if (Model.Count > 0)
{
    <p style="margin-top: 10px">
        The following users spent more than @threshold.ToString("C2")
    </p>
    <ul>
        @foreach (int cmid in Model.Keys)
        {
            <li><a href='@Url.Action("See", "Member")?cmid=@cmid' target="_blank">@cmid</a> - @Model[cmid].ToString("C2")</li>
        }
    </ul>
}